{
  "root": true,
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "node": true
  },
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": "false"
    }
  },
  "rules": {
    "no-unexpected-multiline": 2,
    "semi": [1, "never"],
    "no-var": 1,
    "eqeqeq": 1,
    "dot-notation": 1,
    "no-caller": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-implied-eval": 2,
    "strict": 0,
    "no-multi-spaces": 1,
    "no-console": 1,
    "no-case-declarations": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "no-useless-constructor": 0,
    "space-infix-ops": 0
  },
  "globals": {
    "__filename": true
  }
}
